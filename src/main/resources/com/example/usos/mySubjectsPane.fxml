<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.usos.MySubjectsPaneController">
   <children>
      <TableView fx:id="tableView" layoutX="18.0" layoutY="83.0" prefHeight="305.0" prefWidth="453.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="105.0" text="Name" />
          <TableColumn fx:id="numberOfStudentsColumn" prefWidth="150.0" text="Number Of Students" />
            <TableColumn fx:id="professorNameColumn" prefWidth="140.0" text="Professor's Name" />
        </columns>
      </TableView>
      <Button fx:id="refreshButton" layoutX="506.0" layoutY="83.0" mnemonicParsing="false" onAction="#onRefresh" prefHeight="22.0" prefWidth="65.0" text="Refresh" />
      <Button fx:id="removeButton" layoutX="506.0" layoutY="129.0" mnemonicParsing="false" onAction="#onRemoveButton" prefHeight="25.0" prefWidth="65.0" text="Remove" />
   </children>
</AnchorPane>
